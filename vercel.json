{
  "headers": [
    {
      "source": "/(.*)\\.(js|css|jpg|jpeg|gif|png|svg|txt|ttf|woff2|webmanifest)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=157680000, s-maxage=157680000"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path",
      "destination": "https://codfi.sh/:path"
    }
  ]
}

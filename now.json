{
  "version": 2,
  "name": "codfish.io",
  "alias": "codfish.io",
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    {
      "src": ".*\\.(js|css|jpg|jpeg|gif|png|svg|txt|ttf|woff2)$",
      "headers": {
        "Cache-Control": "public, max-age=157680000, s-maxage=157680000"
      }
    }
  ]
}
